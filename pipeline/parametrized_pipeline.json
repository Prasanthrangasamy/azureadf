{
	"name": "parametrized_pipeline",
	"properties": {
		"activities": [
			{
				"name": "parametrize",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "parameter_dataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"PP_Container": {
				"type": "string"
			},
			"PP_Folder": {
				"type": "string"
			},
			"PP_Filename": {
				"type": "string"
			}
		},
		"annotations": []
	}
}